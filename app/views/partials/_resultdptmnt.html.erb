     <h2 class="page-header">View Results</h2>
        <div id="books-index">

          
          <div class="row">
              <div class="col-md-6 col-sm-6">
                  <table class="table table-striped">
                      <tr>
                        <th>Department</th>
                        <th>Level</th>
                        <th>Action</th>
                      </tr>
                      <% @results.each do |result| %>
                      <tr>
                        <td><%= @department.name %></td>
                        <td><%= result.level %></td>
                        <td><%= link_to 'show marks', admin_result_path(result.id), class:'btn btn-primary' %></td>
                      </tr>
                      <% end %>
                  </table>
              </div>
        </div>
  
